{
    "name": "reti-ui-common",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "build_wasm": "sh ../reti-js/init_test.sh",
        "dev": "pnpm build_wasm && vite",
        "build": "pnpm build_wasm && vite build",
        "preview": "pnpm build_wasm && vite preview",
        "check": "pnpm build_wasm && svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json",
        "lint": "pnpm build_wasm && eslint './src/**/*.{js,ts,svelte}'",
        "test": "pnpm build_wasm && vitest run",
        "test:watch": "pnpm build_wasm && vitest"
    },
    "devDependencies": {
        "@eslint/js": "^9.7.0",
        "@sveltejs/vite-plugin-svelte": "^3.1.1",
        "@testing-library/jest-dom": "^6.4.6",
        "@testing-library/svelte": "^5.2.0",
        "@tsconfig/svelte": "^5.0.4",
        "@types/katex": "^0.16.7",
        "eslint": "^9.7.0",
        "eslint-plugin-svelte": "^2.42.0",
        "happy-dom": "^14.12.3",
        "prettier": "3.3.3",
        "prettier-plugin-svelte": "^3.2.5",
        "svelte": "^4.2.18",
        "svelte-check": "^3.8.1",
        "tslib": "^2.6.3",
        "typescript": "^5.2.2",
        "typescript-eslint": "8.0.0-alpha.44",
        "vite": "^5.3.1",
        "vitest": "^2.0.3"
    },
    "dependencies": {
        "globals": "^15.8.0",
        "katex": "^0.16.11"
    }
}
